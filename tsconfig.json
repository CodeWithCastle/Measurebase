{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/components/assets/icons/people.js",
    "app/components/ui/AppMenuView.jsx",
    "app/contexts/AppStylesContext.jsx",
    "app/contexts/AppThemeContext.jsx"
  ]
}
